    "def isEven(num):\n",
    "    if num % 2 ==0:\n",
    "        return True\n",
    "\n",
    "n = int(input(\"Enter range of number :\"))\n",
    "lst=range(n)\n",
    "\n",
    "output_list=list(filter(isEven,lst))\n",
